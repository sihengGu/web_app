{
    "data": {
        "data_path": "./dataset",
        "seq_length": 128,
        "train_ratio": 0.7,
        "val_ratio": 0.15,
        "test_ratio": 0.15,
        "batch_size": 16,
        "num_workers": 0,
        "shuffle": true,
        "augmentation": false,
        "interpolation": "linear",
        "noise_level": 0.01,
        "scale_range": [
            0.9,
            1.1
        ],
        "time_shift_range": 5
    },
    "model": {
        "model_type": "noisy_baseline",
        "input_dim": 7,
        "hidden_dim": 64,
        "dropout": 0.1,
        "estimate_uncertainty": true,
        "num_blocks": 3,
        "complexity_level": "medium",
        "num_conv_blocks": 2,
        "num_attention_blocks": 2,
        "n_heads": 4,
        "use_ensemble": false,
        "ensemble_method": "mean",
        "n_transformer_layers": 4,
        "use_wavelet": false,
        "use_fpn": false,
        "attention_dropout": 0.1,
        "use_channel_attention": true,
        "use_spatial_attention": true,
        "use_se_block": true,
        "transformer_dim_feedforward": 512,
        "transformer_activation": "relu",
        "transformer_norm_first": false,
        "wavelet_levels": 3,
        "wavelet_type": "db4",
        "fpn_out_channels": 64,
        "fpn_num_levels": 3,
        "noise_config": {
            "input_noise_std": 0.2,
            "feature_noise_std": 0.05,
            "conv_noise_std": 0.06,
            "attention_noise_std": 0.02,
            "lstm_noise_std": 0.04,
            "weight_noise_std": 0.005,
            "adaptive_noise": false,
            "noise_schedule": "constant"
        }
    },
    "training": {
        "epochs": 120,
        "learning_rate": 0.0008,
        "weight_decay": 0.0001,
        "patience": 20,
        "optimizer": "AdamW",
        "scheduler": "ReduceLROnPlateau",
        "loss_type": "uncertainty",
        "gradient_clip_norm": 1.0,
        "gradient_accumulation_steps": 1,
        "warmup_epochs": 5,
        "label_smoothing": 0.0,
        "mixup_alpha": 0.0,
        "cutmix_alpha": 0.0,
        "scheduler_params": {
            "mode": "min",
            "factor": 0.5,
            "patience": 10,
            "verbose": true,
            "min_lr": 1e-06
        },
        "optimizer_params": {
            "betas": [
                0.9,
                0.999
            ],
            "eps": 1e-08,
            "amsgrad": false
        },
        "loss_weights": {
            "mse": 1.0,
            "uncertainty": 1.0,
            "regularization": 0.001
        }
    },
    "experiment": {
        "experiment_name": "ultra_resistivity_inversion",
        "save_dir": "./experiments",
        "save_best_model": true,
        "save_final_model": true,
        "save_history": true,
        "save_predictions": true,
        "save_attention_maps": true,
        "plot_results": true,
        "print_detailed_results": true,
        "seed": 42,
        "deterministic": true,
        "use_tensorboard": true,
        "use_wandb": false,
        "wandb_project": "resistivity_inversion",
        "save_checkpoint_every": 50,
        "max_checkpoints": 5,
        "validate_every": 1,
        "test_during_training": false,
        "mixed_precision": true,
        "gradient_accumulation_steps": 1
    }
}